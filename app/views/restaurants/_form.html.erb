<div>
  <%= form_for @restaurant do |f| %>

    <%= f.label :name %>
    <%= f.text_field :name %>
    <br>
    <%= f.label :address %>
    <%= f.text_field :address %>
    <br>
    <%= f.label :open_hrs %>
    <%= f.text_field :open_hrs %>
    <br>
    <%= f.label :cost_two %>
    <%= f.text_field :cost_two %>
    <br>
    <%= f.label :phone %>
    <%= f.text_field :phone %>
    <br>
    <%= f.label :image %>
    <%= f.text_field :image %>
    <br>
    <%= f.submit %>
  <% end %>
</div>
